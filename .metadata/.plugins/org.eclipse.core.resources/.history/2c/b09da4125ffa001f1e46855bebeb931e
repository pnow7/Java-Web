package practice.sec06;

public class Practice1 {

	public static void main(String[] args) {
		int[] a = {1, 2, 3};
		for(int i = 0; i < a.length; i++) {
			a[i] = a[i] + 2;
			System.out.println("a["+i+"] : " + a[i]);
		}
		int[] b = new int[10];
		for(int i = 0; i < b.length; i++) {
			b[i] = (i+1);
			b[i] *= 3;
			System.out.print(b[i]+" ");
		}
		
	}

}

package test;

import java.util.Scanner;

public class Main4 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] num = new int[9];
		for(int i = 0; i<num.length; i++) {
			num[i] = sc.nextInt();
		}
		for(int j = 0; j<num.length; j++) {
			if(num[0] < num[j]) {
				num[0] = num[j];
			}
			System.out.println(num[0]);
			System.out.println(j+1);
		}


	}

}

package test;
import java.util.Scanner;
public class Main11 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		sc.close();
		
		int cnt = 0;

		while(N != 1) {
			cnt++;
			if(N%3==0) {
				N = N/3;			
			}
			else if(N%2==0) {
				N = N/2;		
			}
			else {
				N--;
			}	
		}
		System.out.println(cnt);
	}
}

package practice.sec02;

public class practice3 {

	public static void main(String[] args) {
		double d = 85.4;
		int score = (int) d;
		System.out.println("score="+score);
		System.out.println("d="+d);
		System.out.println("=======================");
		
		byte a = 10;
		byte b = 30;
		byte c = (byte)(a*b);
		System.out.
		println(c);
		System.out.println("=======================");
		
		double num1 = 1.2345;
		int num2 = 6;
		System.out.println(num1 + num2);
	}

}

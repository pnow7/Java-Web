package test;

import java.util.Scanner;
public class Main14 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] num = new int[N];
		int sum = 0;
		for(int i = 0; i < N; i++) {
			String[] strArr = sc.next().split(""); // 공백없이 입력
		}	
			
		for(int j = 0; j < N; j++) {
			num[j] = Integer.parseInt(strArr[j]); //
			sum +=num[j];
		}
		sc.close();
		System.out.println(sum);

	}

}

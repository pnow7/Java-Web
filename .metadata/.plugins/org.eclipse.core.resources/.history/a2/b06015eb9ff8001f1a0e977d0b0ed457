package practice.sec03;

public class practice42 {

	public static void main(String[] args) {
		byte a1 = 10;
		byte a2;
		a2 = 11;
		System.out.println(a1); //바이트 단위인 a1값 10 출력
		System.out.println(a2); //바이트 단위인 a2값이 11로 저장되어 a2값 11 출력
		System.out.println(a1+a2); //a1은 10 a2는 11이다 10+11하여 21을 출력
		System.out.println(a1+1); //a1은 10에 +1을 하여 11을 출력
		
		short b1 = 10;
		short b2;
		b2 = 11;
		System.out.println(a1); //바이트 단위인 a1값 10 출력
		System.out.println(a2); //바이트 단위인 a2값이 11로 저장되어 a2값 11 출력
		System.out.println(a1+a2); //a1은 10 a2는 11이다 10+11하여 21을 출력
		System.out.println(a1+1); //a1은 10에 +1을 하여 11을 출력

	}

}

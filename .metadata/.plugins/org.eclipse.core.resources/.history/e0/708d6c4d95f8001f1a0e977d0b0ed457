package practice.sec03;

import java.util.Scanner;
public class practice3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		for(int i = 1; i <= 5; i++) {
			System.out.println("*****");
		}
		
		System.out.print("-------------------\n");
		
		String star = "*";
		for(int j = 1; j <= 5; j++) {
			System.out.println(star);
			star += "*";
		}
		
		System.out.print("-------------------\n");
		
		for(int k = 1; k <= 4; k++) {
			System.out.print(" ");
			for(int M = 5-k; M <= 5; M++) {
				System.out.println(star);
				star +="*";
			}
		}

	}

}

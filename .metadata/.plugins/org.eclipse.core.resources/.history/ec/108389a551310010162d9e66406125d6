package tb_api;

import java.util.List;
import com.fasterxml.jackson.annotation.JsonProperty;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor

public class TbDTO {
    
	private List<Item> itemList;
	
    private response response;
    private header header;
    private body body;
    
    @Data
    @NoArgsConstructor
    public static class response {
        private header header;
        private body body;
    }

    @Data
    @NoArgsConstructor
    public static class header {
        private String resultCode;
        private String resultMsg;
        private String type;
    }
    
    @Data
    @NoArgsConstructor
    public static class body {
        private List<Item> items;
        private int totalCount;
        private int numOfRows;
        private int pageNo;
    }
    
    @Data
    @NoArgsConstructor
    public static class Item {
    	@JsonProperty("CROWDED")
    	private String crowded;
    	@JsonProperty("LOWPLATE")
        private String lowPlate;
    	@JsonProperty("PLATENO")
        private String plateNo;
    	@JsonProperty("REMAINSEATCNT")
        private String remainSeatCnt;
    	@JsonProperty("ROUTEID")
        private String routeId;
    	@JsonProperty("ROUTETYPECD")
        private String routeTypeCd;
    	@JsonProperty("STATECD")
        private String stateCd;
    	@JsonProperty("STATIONID")
        private String stationId;
    	@JsonProperty("STATIONSEQ")
        private String stationSeq;
    	@JsonProperty("TAGLESSCD")
        private String taglessCd;
    	@JsonProperty("VEHID")
        private String vehId;
    	

  
        
    }

}